      "@tonaljs/scale-dictionary": "^3.4.4",
      "@tonaljs/tonal": "^3.6.0",
      "axios": "^0.19.2",
      "core-js": "^3.6.4",
      "promise": "^8.1.0",
      "prop-types": "^15.7.2",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "regenerator-runtime": "^0.13.5",
      "soundfont-player": "^0.11.0",
      "styled-components": "^4.4.1",
