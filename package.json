{
  "name": "choordinator",
  "version": "0.3.0",
  "description": "Scale and chord inventory for browsers",
  "repository": "github:sai-chai/choordinator",
  "main": "index.js",
  "scripts": {
    "start": "node ./scripts/prod.server.js",
    "start:prod": "npm run lint && nodemon ./scripts/prod.server.js",
    "start:dev": "npm run lint && node ./scripts/dev.server.js",
    "build": "npm run lint && webpack --config ./webpack/prod.config.js",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "test": "rimraf ./coverage && npm run lint && jest --coverage",
    "test:watch": "jest --watch --coverage --lastCommit"
